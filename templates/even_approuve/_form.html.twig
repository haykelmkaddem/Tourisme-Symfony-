<div class="row">
    <div class="col-md-1"></div>
    <div class="col-md-6">
{{ form_row(form.descriptionEven,{'attr': {'class': 'form-control , col-md-12 form-group p_star'}})  }}
    </div>
    <div class="col-md-2">
{{ form_row(form.heureDebutEven,{'attr': {'class': 'form-control , heureEven'}})  }}
