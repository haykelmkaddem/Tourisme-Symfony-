{{ form_row(form.titreEven, {'attr': {'class': 'form-control , col-md-12 form-group p_star'}})  }}
    </div>

    <div class="col-md-3">
{{ form_row(form.dateDebutEven,{'attr': {'class': 'form-control , dateEven'}})  }}
    </div>
    <div class="col-md-3">
{{ form_row(form.dateFinEven,{'attr': {'class': 'form-control , dateEven'}})  }}
