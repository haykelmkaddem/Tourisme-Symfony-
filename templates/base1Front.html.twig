                        </div>
                        <div class="col-6">
                            <a href="#" class="image fit"><img src="{{ asset('images/pic15.jpg') }}" alt="" /></a>
                        </div>
                    </div>
                </section>

            </div>
            <hr />
            <div class="row">
                <div class="col-12">

                    <!-- Contact -->
                    <section class="contact">
